{
  "name": "@modele/core",
  "version": "0.0.0",
  "scripts": {
    "build": "tsup-node",
    "dev": "tsup-node --watch",
    "lint": "eslint \"**/*.ts*\""
  },
  "devDependencies": {
    "@modele/eslint-config": "workspace:*",
    "@modele/tsconfig": "workspace:*",
    "@types/express": "^4.17.17",
    "@types/node": "^17.0.12",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/webpack": "^5.28.1",
    "eslint": "^8.43.0",
    "next": "^13.4.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsup": "^7.0.0",
    "typescript": "^4.5.2"
  },
  "peerDependencies": {
    "next": "^13.4.6",
    "react": "^18.2.0"
  },
  "tsup": {
    "entry": [
      "index.ts"
    ],
    "splitting": false,
    "sourcemap": true,
    "clean": true,
    "dts": true,
    "format": [
      "cjs",
      "esm"
    ]
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "dependencies": {
    "chalk": "^4.1.2"
  },
  "type": "module"
